<VirtualHost *:80>
  ServerName app.local
# DocumentRoot /vagrant/webroot/drupal7
  DocumentRoot /var/www/drupal7
# <Directory /vagrant/webroot/drupal7>
  <Directory /var/www/drupal7>
    DirectoryIndex index.php index.html
    AllowOverride All
    Order allow,deny
    Allow from all
  </Directory>
</VirtualHost>


